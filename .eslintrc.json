{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha" : true,
        "node":true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["off", "always"],
        "quotes": ["off", "double"],
        "no-unused-vars":["off", "always"],
        "no-prototype-builtins":["off","always"]
    }
}
